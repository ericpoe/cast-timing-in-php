{
    "name": "ericpoe/cast-timing-in-php",
    "description": "Test PHP casting options and compare like-for-like timings",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Eric Poe",
            "email": "eric.poe@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "doctrine/annotations": "^1.14.3",
        "doctrine/doctrine-bundle": "^2.7.2",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^2.19",
        "league/csv": "^9.6.2",
        "symfony/console": "~5.4.36",
        "symfony/dotenv": "~5.4.35",
        "symfony/flex": "^1.21.6 || ^2",
        "symfony/framework-bundle": "~5.4.36",
        "symfony/runtime": "~5.4.35",
        "symfony/stopwatch": "5.4.*",
        "symfony/yaml": "~5.4.35"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.19.0",
        "phpstan/extension-installer": "^1.3.1",
        "phpstan/phpstan": "^1.10.59",
        "phpstan/phpstan-doctrine": "^1.3.62",
        "phpstan/phpstan-symfony": "^1.3.8",
        "phpunit/phpunit": "^8.5.37",
        "rector/rector": "^1.0.2",
        "squizlabs/php_codesniffer": "^3.9",
        "symfony/browser-kit": "5.4.*",
        "symfony/css-selector": "5.4.*",
        "symfony/maker-bundle": "^1.43",
        "symfony/phpunit-bridge": "^7.0.4"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "ergebnis/composer-normalize": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "platform": {
            "php": "7.2.34"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "5.4.*"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts",
            "composer bump"
        ],
        "post-update-cmd": [
            "@auto-scripts",
            "composer bump"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "pre-git-push": [
            "composer validate",
            "composer normalize",
            "bin/console cache:clear",
            "rector",
            "phpcbf",
            "phpcs",
            "phpstan analyze --memory-limit=-1",
            "bin/phpunit"
        ]
    }
}
